{
    "log": {
        "disabled": false,
        "level": "warn",
        "output": "./sing-box.log",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "default-dns",
                "address": "192.168.31.2",
                "detour": "direct-out"
            }, 
            {
                "tag": "system-dns",
                "address": "local",
                "detour": "direct-out"
            }, 
            {
                "tag": "block-dns",
                "address": "rcode://name_error"
            }
        ],
        "rules": [
            {
                "outbound": "any",
                "server": "default-dns"
            }, 
            {
                "query_type": "HTTPS",
                "server": "block-dns"
            }, 
            {
                "clash_mode": "direct",
                "server": "default-dns"
            }, 
            {
                "clash_mode": "global",
                "server": "default-dns"
            }
        ],
        "strategy": "ipv4_only",
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": false,
        "final": "default-dns"
    },
    "inbounds": [
        {
            "type": "direct",
            "tag": "dns-in",
            "listen": "::",
            "listen_port": 5333
        }, 
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "::",
            "listen_port": 5330,
            "sniff": true,
            "sniff_override_destination": false,
            "set_system_proxy": false
        }, 
        {
            "type": "redirect",
            "tag": "redirect-in",
            "listen": "::",
            "listen_port": 5331,
            "sniff": true,
            "sniff_override_destination": false
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct-out",
            "routing_mark": 100
        }, 
        {
            "type": "block",
            "tag": "block-out"
        }, 
        {
            "type": "dns",
            "tag": "dns-out"
        }, 
        {
            "type": "socks",
            "tag": "socks-out",
            "server": "proxy.local",
            "server_port": 7891,
            "version": "5",
            "network": "udp"
        }
    ],
    "route": {
        "rules": [
            {
                "inbound": "dns-in",
                "outbound": "dns-out"
            }, 
            {
                "protocol": "dns",
                "outbound": "dns-out"
            }, 
            {
                "protocol": "quic",
                "outbound": "block-out"
            }, 
            {
                "clash_mode": "direct",
                "outbound": "direct-out"
            }, 
            {
                "rule_set": [
                    "netflix",
                    "youtube",
                    "ai",
                    "proxy"
                ],
                "outbound": "socks-out"
            }
        ],
        "rule_set": [
          {
            "tag": "netflix",
            "type": "remote",
            "format": "binary",
            "url": "https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/netflix.srs"
          },
          {
            "tag": "youtube",
            "type": "remote",
            "format": "binary",
            "url": "https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/youtube.srs"
          },
          {
            "tag": "ai",
            "type": "remote",
            "format": "binary",
            "url": "https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/ai.srs"
          },
          {
            "tag": "proxy",
            "type": "remote",
            "format": "binary",
            "url": "https://github.com/DustinWin/ruleset_geodata/releases/download/sing-box-ruleset/proxy.srs"
          }
        ],
        "auto_detect_interface": true,
        "final": "direct-out"
    },
    "experimental": {
        "cache_file": {
            "enabled": true,
            "path": "./cache.db"
        },
        "clash_api": {
            "external_controller": "127.0.0.1:9090",
            "external_ui": "./ui/",
            "external_ui_download_detour": "socks-out",
            "default_mode": "rule"
        }
    }
}

