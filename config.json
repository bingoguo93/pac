{
    "log": {
        "disabled": false,
        "level": "info",
        "output": "sing-box.log",
        "timestamp": true
    },
    "inbounds": [
        {
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "0.0.0.0",
            "listen_port": 7891
        },
        {
            "type": "redirect",
            "tag": "redirect-in",
            "listen": "::",
            "listen_port": 5331
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct-out"
        },
        {
            "type": "socks",
            "tag": "socks-out",
            "server": "192.168.31.82",
            "server_port": 7891,
            "username": "",
            "password": "",
            "version": "5",
            "network": "tcp",
            "connect_timeout": "5s"
        }
    ],
    "route": {
        "rules": [
           {
             "domain": [
               "edgt.abram.dynv6.net"
             ],
             "domain_suffix": [
               ".cn"
             ],
             "domain_regex": [
               "^stun\\..+"
             ],
             "geosite": [
               "cn"
             ],
             "source_geoip": [
               "private"
             ],
             "geoip": [
               "cn"
             ],
             "outbound": "direct-out"
           },
           {
             "clash_mode": "Global",
             "outbound": "socks-out"
           }
        ],
    "auto_detect_interface": true,
    "final": "socks-out"
    },
  "experimental": {
      "cache_file": {
        "enabled": true,
        "path": "cache.db"
    },
      "clash_api": {
        "external_ui": "ui",
        "external_controller": "0.0.0.0:9090",
        "external_ui_download_url": "https://ghproxy.net/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
        "external_ui_download_detour": "socks-out",
        "default_mode": "Enhanced"
    }
  }
}
