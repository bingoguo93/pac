{
    "log": {
        "disabled": false,
        "level": "info",
        "output": "sing-box.log",
        "timestamp": true
    },
    "dns": {
        "servers": [
	   {
                "tag": "default-dns",
                "address": "223.5.5.5",
                "detour": "direct-out"
            },
	    {
                "tag": "block-dns",
                "address": "rcode://name_error"
            }, 
			{
                "tag": "cloudflare",
                "address": "https://1.1.1.1/dns-query",
                "address_resolver": "default-dns",
                "address_strategy": "ipv4_only",
                "strategy": "ipv4_only",
                "client_subnet": "1.0.1.0"
            }
        ],
        "rules": [
		    {
                "outbound": "any",
                "server": "default-dns"
            }, 
			{
                "query_type": "HTTPS",
                "server": "block-dns"
            }, 
			{
                "clash_mode": "direct",
                "server": "default-dns"
            }, 
			{
                "clash_mode": "global",
                "server": "default-dns"
            }, 
			{
                "rule_set": "proxy",
                "server": "cloudflare"
            }
        ],
        "strategy": "ipv4_only",
        "disable_cache": false,
        "disable_expire": false,
        "independent_cache": false,
        "final": "default-dns"
    },
    "inbounds": [
	    {
            "type": "direct",
            "tag": "dns-in",
            "listen": "::",
            "listen_port": 5333
        }, 
		{
            "type": "mixed",
            "tag": "mixed-in",
            "listen": "::",
            "listen_port": 7891,
            "sniff": true,
            "sniff_override_destination": false,
            "set_system_proxy": false
        }, 
		{
            "type": "redirect",
            "tag": "redirect-in",
            "listen": "::",
            "listen_port": 5331,
            "sniff": true,
            "sniff_override_destination": false
        }
    ],
    "outbounds": [
	   {
            "type": "direct",
            "tag": "direct-out",
            "routing_mark": 100
        }, 
		{
            "type": "block",
            "tag": "block-out"
        }, 
		{
            "type": "dns",
            "tag": "dns-out"
        }, 
		{
            "type": "socks",
            "tag": "socks-out",
            "server": "192.168.31.82",
            "server_port": 7891,
            "username": "",
            "password": "",
            "version": "5",
            "network": "tcp"
        }
    ],
    "route": {
        "rules": [
          {
            "type": "logical",
            "mode": "or",
            "rules": [
              {
                "protocol": "dns"
              },
              {
                "port": 53
              }
            ],
            "outbound": "dns-out"
          },
          {
            "ip_is_private": true,
            "outbound": "direct-out"
          },
          {
            "clash_mode": "Direct",
            "outbound": "direct-out"
          },
          {
            "clash_mode": "Global",
            "outbound": "socks-out"
          },
          {
            "type": "logical",
            "mode": "or",
            "rules": [
              {
                "port": 853
              },
              {
                "network": "udp",
                "port": 443
              },
              {
                "protocol": "stun"
              }
            ],
            "outbound": "block-out"
          },
          {
            "rule_set": [
              "proxy"
            ],
            "outbound": "socks-out"
          }
    ],
    "rule_set": [
      {
        "tag": "proxy",
        "type": "remote",
        "format": "binary",
        "url": "https://gitee.com/abram-guo/pac/raw/master/proxy.srs"
      }
    ],
    "auto_detect_interface": true,
    "final": "direct-out"
    },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "cache.db",
      "store_fakeip": true
    },
    "clash_api": {
      "external_ui": "ui",
      "external_controller": "0.0.0.0:9090",
      "external_ui_download_url": "https://ghproxy.net/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "direct-out",
      "default_mode": "Enhanced"
    }
  }
}
